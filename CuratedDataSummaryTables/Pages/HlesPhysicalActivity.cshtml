@page
@{
    ViewData["Title"] = "HLES Physical Activity";
}

@{
    // Get selected year from the query string and build the file path
    var year = HttpContext.Request.Query["year"].ToString() ?? Helpers.Constants.defaultDataYear;
    var filePath = $"/DataViews/{year}/HlesPhysicalActivity.html";
}

<html-embed file-path="@filePath"></html-embed>